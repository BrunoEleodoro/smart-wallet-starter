{
  "name": "@asgarovf/smart-wallet",
  "description": "Smart Wallet SDK for ZKsync",
  "version": "0.0.1",
  "devDependencies": {
    "@passwordless-id/webauthn": "1.5.0",
    "@types/jest": "29.5.3",
    "@types/node": "16.0.0",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "cbor": "9.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "ethers": "5.7.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "^5",
    "zksync-ethers": "5.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "keywords": [
    "zksync",
    "smart-wallet",
    "sdk"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "maintainers": [
    {
      "name": "Farhad Asgarov",
      "email": "farhad@clave.team"
    }
  ],
  "private": false,
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "tsc --watch",
    "format": "prettier --write \"./**/*.ts\"",
    "lint": "eslint \"./**/*.ts\" --fix",
    "lint-check": "eslint \"./**/*.ts\"",
    "release": "npm run build && npm publish",
    "test": "jest --max-workers 4 --verbose"
  },
  "types": "src/index.ts",
  "dependencies": {
    "axios": "^1.7.7",
    "buffer": "^6.0.3"
  }
}
